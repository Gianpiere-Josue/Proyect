<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>ReservaServlet</servlet-name>
        <servlet-class>com.utp.proceso1.controlador.ReservaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServerlet</servlet-name>
        <servlet-class>com.utp.proceso1.controlador.LoginServerlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReservaServlet</servlet-name>
        <url-pattern>/ReservaServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ContactoServlet</servlet-name>
        <servlet-class>com.utp.proceso1.controlador.ContactoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContactoServlet</servlet-name>
        <url-pattern>/ContactoServlet</url-pattern>
    </servlet-mapping>
</web-app>
